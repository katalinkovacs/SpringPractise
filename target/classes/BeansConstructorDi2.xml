        <constructor-arg type="java.lang.String" value="New Zealand"/>
    </bean>  -->

    <bean id="user5" class="spring.dependencyinjection.constructorbased2.User" >
        <constructor-arg index="1" value="41"/>
        <constructor-arg index="0" value="Zoltan" />
        <constructor-arg index="2" value="Australia"/>
