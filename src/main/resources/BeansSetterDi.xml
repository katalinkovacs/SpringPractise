<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="employee" class="spring.dependencyinjection.setterbased.Employee">

        <property name="id">
            <value>20</value>
        </property>

        <property name="name">
            <value>Sebastian</value>
        </property>

        <property name="city">
            <value>Altona Meadows</value>
        </property>

    </bean>

    <bean id="employee2" class="spring.dependencyinjection.setterbased.Employee" >
        <constructor-arg value="33" />
        <constructor-arg value="Katalin"/>
        <constructor-arg value="Lower Hutt"/>
    </bean>

</beans>